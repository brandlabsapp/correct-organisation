  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node --max-old-space-size=384 dist/src/main",
    "dev": "nest start --watch",
    "dev:type": "nest start -b swc --type-check",
    "sync": "SYNC=true npm run dev",
    "force": "FORCE=true npm run dev",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "analyse": "time tsc --diagnostics -p tsconfig.json",
    "db:migrate": "npx ts-node ./node_modules/.bin/sequelize-cli db:migrate --config src/config/database.config.ts --migrations-path db/migrations"
  },
